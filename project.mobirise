            "parallax": true,
            "bgImageRadio": true,
            "bgImage": "@PROJECT_PATH@/assets/images/full-unsplash-photo-1431162020154-5eb22b2456d6-2000x1500-19.jpg",
            "bgColor": "#a49062",
            "bgColorRadio": false,
